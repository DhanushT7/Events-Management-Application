{
  "name": "events-management-application",
  "version": "1.0.0",
  "description": "A comprehensive web application for managing educational events, training programs, workshops, and certificate courses at Anna University with role-based access and professional document generation.",
  "main": "index.js",
  "scripts": {
    "dev": "npm run backend",
    "backend": "cd Backend && npm run dev",
    "frontend": "cd Frontend && npm run dev",
    "install-all": "npm install && cd Backend && npm install && cd ../Frontend && npm install",
    "build": "cd Frontend && npm run build",
    "start": "npm run backend",
    "test": "cd Backend && npm test && cd ../Frontend && npm test",
    "clean": "rm -rf node_modules Backend/node_modules Frontend/node_modules",
    "setup": "npm run install-all"
  },
  "keywords": [
    "events-management",
    "anna-university",
    "education",
    "training-programs",
    "certificate-courses",
    "brochure-generation",
    "document-management",
    "react",
    "nodejs",
    "mongodb",
    "pdf-generation",
    "role-based-access"
  ],
  "author": "Dhanush T <dhanush@annauniv.edu>",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "bcrypt": "^6.0.0",
    "concurrently": "^9.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.17.0",
    "mongoose": "^8.15.1",
    "multer": "^2.0.1",
    "nodemailer": "^7.0.3",
    "nodemon": "^3.1.10",
    "number-to-words": "^1.2.4",
    "pdfkit": "^0.17.1",
    "react-router-dom": "^7.6.2",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "jest": "^27.5.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DhanushT7/Events-Management-Application.git"
  },
